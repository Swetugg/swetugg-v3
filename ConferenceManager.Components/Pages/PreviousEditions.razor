@page "/previous-editions"
@inject ConferenceService ConferenceService

<TextArticle Header="Previous Edtions" Text="" Buttons="@previousEditionsLinks"></TextArticle>

@code {
    Button[]? previousEditionsLinks;
    protected override async Task OnInitializedAsync()
    {
        var Conferences = await ConferenceService.GetAllEditionssAsync();
        previousEditionsLinks = Conferences.Where(e => !e.IsCurrent).Select(c => new Button($"{c.City} {c.Year}", $"/{c.City}/{c.Year}")).ToArray();
    }
}